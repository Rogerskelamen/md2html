{
  "name": "@rokelamen/md2html",
  "type": "module",
  "version": "0.1.1",
  "description": "A simple tool to convert markdown content to html",
  "author": "rokelamen <rogerskelamen@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "md2html": "./bin/cli.cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf bin dist",
    "build": "npm run clean && rollup -c rollup.config.ts --configPlugin typescript",
    "test": "node bin/cli.cjs",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "typescript",
    "md2html"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/commander": "^2.12.0",
    "@types/node": "^25.0.3",
    "commander": "^14.0.2",
    "rimraf": "^6.1.2",
    "rollup": "^4.54.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
